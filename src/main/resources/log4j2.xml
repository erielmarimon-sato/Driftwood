<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="MyConsole" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5level | %d{yyy-MM-dd HH:mm:ss.SSS} | %-30logger{2} | %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="MyConsole" />
        </Root>
    </Loggers>
</Configuration>